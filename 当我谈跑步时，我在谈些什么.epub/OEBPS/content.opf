        <dc:title>当我谈跑步时，我在谈些什么</dc:title>
        <dc:creator opf:file-as="施小炜　译" opf:role="aut">[日]村上春树</dc:creator>
        <dc:language>zh</dc:language>
        <dc:subject>外国文学</dc:subject>
